{
  "build": {
    "core": "maple",
    "cpu": "cortex-m3",
    "extra_flags": "-Wno-cpp -Wno-unused-variable -std=c++11 -O2 -DARDUINO_GENERIC_STM32F103R -DMCU_STM32F103RE ",
    "f_cpu": "72000000L",
    "hwids": [
        ["0x1EAF", "0x0003"],
        ["0x1EAF", "0x0004"]
    ],
    "ldscript": "stm32f103re.ld",
    "mcu": "stm32f103re",
    "variant": "generic_stm32f103r"
  },
  "frameworks": ["arduino"],
  "name": "Truckduino automatic control systems",
  "upload": {
    "disable_flushing": false,
    "maximum_ram_size": 65536,
    "maximum_size": 524288,
    "protocol": "serial",
    "require_upload_port": true,
    "use_1200bps_touch": false,
    "wait_for_upload_port": true
  },
  "url": "https://github.com/microseti/truckduino",
  "vendor": "Microseti Ltd."
}
